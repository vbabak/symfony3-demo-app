{#
https://getbootstrap.com/docs/3.3/components/#pagination
#}
<p>Displaying <b>{{ current_page }}</b> page out of <b>{{ total_pages }}</b> pages</p>
<ul class="pagination">
    {% for page in links_range %}
        <li class="{% if page == current_page %}active{% endif %}"><a href="{{ url(app.request.get('_route'),{'page': page}) }}">{{ page }}</a></li>
    {% endfor %}
</ul>